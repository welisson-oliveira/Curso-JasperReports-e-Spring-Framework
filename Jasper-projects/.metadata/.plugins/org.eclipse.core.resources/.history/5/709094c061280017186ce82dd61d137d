	<queryString>
		<![CDATA[select 
c.id as id_contato, c.nome as nome, c.endereco_id as endereco_id,
e.id as id_endereco, e.bairro as bairro, e.cidade as cidade, e.logradouro as logradouro,
e.numero as numero
 from contatos c, enderecos e where c.endereco_id = e.id]]>
